<p id="notice"><%= notice %></p>

<p>
  <strong>Feed:</strong>
  <%= @cat.feed_id %>
</p>

<p>
  <strong>Name:</strong>
  <%= @cat.name %>
</p>

<p>
  <strong>Address:</strong>
  <%= @cat.address %>
</p>

<%= content_tag(:iframe, 'map', src:"https://www.google.com/maps/embed/v1/place?key=#{ENV["GOOGLE_API_KEY"]}&q=" + @cat.address, width:"100%", height:320, frameborder: 0) %>
<br>

<%= link_to 'Delete', @cat, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %>
<%= link_to 'Edit', edit_cat_path(@cat), class:"btn btn-success" %>
<%= link_to 'Back', cats_path, class: 'btn btn-default' %>
